#### Work Servers
Host *.placeiq.net
  user amerenda
  ForwardAgent yes
  identityfile ~/.ssh/id_rsa_amerenda_placeiq

host nginx-*.nym1.placeiq.net
  user amerenda
  proxycommand ssh -A amerenda@amerenda-167.nym1.placeiq.net nc %h %p
  ForwardAgent yes
  identityfile ~/.ssh/id_rsa_amerenda_placeiq

